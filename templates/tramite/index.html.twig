{% extends 'base.html.twig' %}

{% block title %}Tramite index{% endblock %}

{% block body %}
    <h1>Tramite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tramite in tramites %}
            <tr>
                <td>{{ tramite.id }}</td>
                <td>
                    <a href="{{ path('tramite_show', {'id': tramite.id}) }}">show</a>
                    <a href="{{ path('tramite_edit', {'id': tramite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('tramite_new') }}">Create new</a>
{% endblock %}

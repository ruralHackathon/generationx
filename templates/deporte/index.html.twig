{% extends 'base.html.twig' %}

{% block title %}Deporte index{% endblock %}

{% block body %}
    <h1>Deporte index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for deporte in deportes %}
            <tr>
                <td>{{ deporte.id }}</td>
                <td>
                    <a href="{{ path('deporte_show', {'id': deporte.id}) }}">show</a>
                    <a href="{{ path('deporte_edit', {'id': deporte.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('deporte_new') }}">Create new</a>
{% endblock %}

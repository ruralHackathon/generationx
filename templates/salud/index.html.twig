{% extends 'base.html.twig' %}

{% block title %}Salud index{% endblock %}

{% block body %}
    <h1>Salud index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for salud in saluds %}
            <tr>
                <td>{{ salud.id }}</td>
                <td>
                    <a href="{{ path('salud_show', {'id': salud.id}) }}">show</a>
                    <a href="{{ path('salud_edit', {'id': salud.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('salud_new') }}">Create new</a>
{% endblock %}
